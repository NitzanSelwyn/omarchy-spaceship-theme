  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#9b9bc7"
      }
    },
    {
      "name": "String",
      "scope": ["string.quoted", "string.template"],
      "settings": {
        "foreground": "#51cf66"
      }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#ffd43b"
      }
    },
    {
      "name": "Built-in constant",
      "scope": ["constant.language"],
      "settings": {
        "foreground": "#9370db"
      }
    },
    {
      "name": "User-defined constant",
      "scope": ["constant.character", "constant.other"],
      "settings": {
        "foreground": "#dda0dd"
      }
    },
    {
      "name": "Variable",
      "scope": ["variable"],
      "settings": {
        "foreground": "#e6e6fa"
      }
    },
    {
      "name": "Keyword",
      "scope": ["keyword"],
      "settings": {
        "foreground": "#9370db"
      }
    },
    {
      "name": "Storage",
      "scope": ["storage"],
      "settings": {
        "foreground": "#ff6b7d"
      }
    },
    {
      "name": "Storage type",
      "scope": ["storage.type"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#4dabf7"
      }
    },
    {
      "name": "Class name",
      "scope": ["entity.name.class", "entity.name.type.class"],
      "settings": {
        "fontStyle": "underline",
        "foreground": "#ffd43b"
      }
    },
    {
      "name": "Function name",
      "scope": ["entity.name.function"],
      "settings": {
        "foreground": "#4dabf7"
      }
    },
    {
      "name": "Function argument",
      "scope": ["variable.parameter"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#f5a97f"
      }
    },
    {
      "name": "Tag name",
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#ff6b7d"
      }
    },
    {
      "name": "Tag attribute",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#ffd43b"
      }
    },
    {
      "name": "Library function",
      "scope": ["support.function"],
      "settings": {
        "foreground": "#22b8cf"
      }
    },
    {
      "name": "Library constant",
      "scope": ["support.constant"],
      "settings": {
        "foreground": "#dda0dd"
      }
    },
    {
      "name": "Library class/type",
      "scope": ["support.type", "support.class"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#4dabf7"
      }
    },
    {
      "name": "Library variable",
      "scope": ["support.other.variable"],
      "settings": {
        "foreground": "#e6e6fa"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid"],
      "settings": {
        "background": "#ff6b7d",
        "foreground": "#1a1a2e"
      }
    },
    {
      "name": "Invalid deprecated",
      "scope": ["invalid.deprecated"],
      "settings": {
        "background": "#dda0dd",
        "foreground": "#1a1a2e"
      }
    }
  ]
}
